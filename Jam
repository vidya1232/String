public class Jam {

	public static void main(String[] args) {
	String name ="vidyamurugan";
	char [] c = name.toCharArray();// v i d y a
	int j=0;
char[] a= new char[c.length];
	for(int i = c.length-1;i>=0;i--)
	{a[j]=c[i];
		j++;	
	}
	String name1 = new String(a);
	System.out.println(name1);

	}
	
	}
